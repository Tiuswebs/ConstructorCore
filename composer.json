{
    "name": "tiuswebs/constructor_core",
    "description": "Constructor Core for tiuswebs",
    "keywords": [
        "laravel",
        "admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Escobar",
            "email": "carlosescobar@weblabor.mx"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravelcollective/html": ">=5",
        "livewire/livewire": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Tiuswebs\\ConstructorCore\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tiuswebs\\ConstructorCore\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
