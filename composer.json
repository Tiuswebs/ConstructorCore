{
    "name": "tiuswebs/constructor_core",
    "description": "Constructor Core for tiuswebs",
    "keywords": [
        "laravel",
        "admin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Escobar",
            "email": "carlosescobar@weblabor.mx"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravelcollective/html": ">=5",
        "laravel/framework": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "autoload": {
        "psr-4": {
            "Tiuswebs\\ConstructorCore\\": "src/",
            "Tiuswebs\\ConstructorCore\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tiuswebs\\ConstructorCore\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.22",
        "spatie/laravel-package-tools": "^1.9.2"
    }
}
